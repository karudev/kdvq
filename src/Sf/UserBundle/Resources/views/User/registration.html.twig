<form name="registration" action="{{ path("front_registration") }}" method="post" >
  
  
    <div class="form-group col-md-12">
        <div class="col-md-4">{{form_label(form.email)}}</div>
        <div class="col-md-6">{{form_widget(form.email)}}</div>
        <div class="col-md-4">{{form_errors(form.email)}}</div>
       
    </div>
     <div class="form-group col-md-12">
         <div class="col-md-4"><label>Mot de passe</label></div>
    <div class="col-md-6">{{form_widget(form.plainPassword)}}</div>
    <div class="col-md-4">{{form_errors(form.plainPassword)}}</div>
     </div>
    <div class="clearfix"></div>
    <button class="submit" type="submit" id="_submit" name="_submit" >Valider</button>

    {{form_rest(form)}}
</form>