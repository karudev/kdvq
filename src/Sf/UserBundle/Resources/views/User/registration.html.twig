<form name="registration" action="{{ path("front_registration") }}" method="post" >
    <div class="form-group">
        {{form_row(form.email)}}
    </div>
    <div class="form-group">
        {{form_row(form.plainPassword)}}
    </div>

    <div><button class="submit" type="submit" id="_submit" name="_submit" >{{ 'Valider'|trans }}</button>
    </div>
    {{form_rest(form)}}
</form>